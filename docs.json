{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Dualship",
  "llmsTxt": true,
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#E57333",
    "light": "#E57333",
    "dark": "#0A0A0A"
  },
  "openapi": "openapi.json",
  "api": {
    "baseUrl": "https://api.dualship.run"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": ["introduction", "quickstart", "flow-structure", "pricing", "project-settings", "secrets"]
          },
          {
            "group": "Triggers",
            "pages": [
              "triggers/http",
              "triggers/queue",
              "triggers/cron"
            ]
          },
          {
            "group": "Nodes",
            "pages": [
              "nodes/overview",
              "nodes/request",
              "nodes/http",
              "nodes/enqueue",
              "nodes/set",
              "nodes/transform",
              "nodes/condition",
              "nodes/switch",
              "nodes/parallel",
              "nodes/loop",
              "nodes/try",
              "nodes/response",
              "nodes/abort",
              "nodes/delay",
              "nodes/log"
            ]
          },
          {
            "group": "Reference",
            "pages": [
              "reference/templates",
              "reference/pipes",
              "reference/validation-rules"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "auth": "required",
        "groups": [
          {
            "group": "Overview",
            "pages": ["api-reference/overview"]
          },
          {
            "group": "Endpoints",
            "openapi": "/swagger.json"
          }
        ]
      }
    ]
  },
  "sso": {
    "enabled": true
  },
  "search": {
    "prompt": "Looking for something ?"
  },
  "footerSocials": {
    "github": "https://github.com/dualship"
  },
  "interaction": {
    "copyPage": true
  }
}
